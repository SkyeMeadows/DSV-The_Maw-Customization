<?xml version="1.0" encoding="utf-8"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <CubeBlocks>
        <Definition xsi:type="MyObjectBuilder_HydrogenEngineDefinition">
            <Id>
                <TypeId>HydrogenEngine</TypeId>
                <SubtypeId>MA_Plasma_Generator</SubtypeId>
            </Id>
            <MaxPowerOutput>1000</MaxPowerOutput>
            <FuelInfos>
                <FuelInfo>
                    <Id>
                        <TypeId>GasProperties</TypeId>
                        <SubtypeId>Plasma</SubtypeId>
                    </Id>
                </FuelInfo>
            </FuelInfos>
        </Definition>
        <Definition xsi:type="MyObjectBuilder_GasTankDefinition">
            <Id>
                <TypeId>OxygenTank</TypeId>
                <SubtypeId>MA_Plasma_Tank</SubtypeId>
            </Id>
            <Capacity>250000</Capacity>
            <StoredGasId>
                <TypeId>GasProperties</TypeId>
                <SubtypeId>Plasma</SubtypeId>
            </StoredGasId>
        </Definition>
    </CubeBlocks>

    <GasProperties>
        <Gas>
            <Id>
                <TypeId>MyObjectBuilder_GasProperties</TypeId>
                <SubtypeId>Plasma</SubtypeId>
            </Id>
            <EnergyDensity>60</EnergyDensity>  <!-- MWh/litres -->
        </Gas>
    </GasProperties>

    <Blueprints>
        <Blueprint>
            <Id>
                <TypeId>BlueprintDefinition</TypeId>
                <SubtypeId>OreToPlasmaFuel</SubtypeId>
            </Id>
            <Prerequisites>
                <Item Amount="1" TypeId="Ingot" SubtypeId="Uranium" />
                <Item Amount="10" TypeId="Ingot" SubtypeId="Nickel"/> 
            </Prerequisites>
            <Result Amount="10.0" TypeId="Ore" SubtypeId="PlasmaFuel" />
        </Blueprint>

        <Blueprint>
            <Id>
                <TypeId>BlueprintDefinition</TypeId>
                <SubtypeId>OreToPlasmaFuel_B</SubtypeId>
            </Id>
            <Public>false</Public>
            <Enabled>false</Enabled>
        </Blueprint>

        <Blueprint>
            <Id>
                <TypeId>BlueprintDefinition</TypeId>
                <SubtypeId>OreToPlasmaFuel_C</SubtypeId>
            </Id>
            <Public>false</Public>
            <Enabled>false</Enabled>
        </Blueprint>

        <Blueprint>
            <Id>
                <TypeId>BlueprintDefinition</TypeId>
                <SubtypeId>OreToPlasmaFuel_D</SubtypeId>
            </Id>
            <Public>false</Public>
            <Enabled>false</Enabled>
        </Blueprint>
		
    </Blueprints>

</Definitions>
